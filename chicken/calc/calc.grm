(require-extension lalr)
(define expr-parser
 (lalr-parser

  (output: calc-parser "calc.yy.scm")

  ; Terminal symbols
  (
   (left: + -)
   (left: * /)
   (nonassoc: uminus))
  (e (e + e)              : (+ $1 $3)
     (e - e)              : (- $1 $3)
     (e * e)              : (* $1 $3)
     (e / e)              : (/ $1 $3)
     (- e (prec: uminus)) : (- $2))))
