  default-language:    Haskell2010

library
  build-depends:       base >=4.7 && <4.8, directory >=1.2 && <1.3
                       , async >=2.0 && <2.1
                       , split >=0.2 && <0.3, HTTP >=4000.2 && <4000.3
                       , time >= 1.4
                       , MissingH >= 1.3
                       , unix-time >=0.3
                       , old-locale >= 1.0

  exposed-modules: Ssah.Ssah, Ssah.Yahoo, Ssah.Utils
  default-language:    Haskell2010




